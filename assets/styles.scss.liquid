/* Size info popup CSS AP 27-09-2022 */
.pplr-wrapper label{cursor: auto !important;}
.pp_size_context {background: rgba(0,0,0, 0.5); left:0; opacity:0; position: fixed; top: 0; transition: opacity 0.5s, visibility 0.5s; visibility: hidden;
  width: 100%; height: 100%; z-index: 40000;}

.js-menu {position: absolute; height: 100%; width:100%; max-width:100%; padding: 12px; margin: 0; background: #fff; z-index:99; overflow-y:auto;
  -webkit-overflow-scrolling: touch; -ms-overflow-style: -ms-autohiding-scrollbar; transition: background 0.5s, transform 0.5s; display:none;}
.js-menu--right {-webkit-transform: translateX(100%); transform: translateX(100%); position: absolute; right: 0;}

.hidden{display:none;}
.js-menu__close {cursor: pointer;}

.pp_sizechart_container .pp_close-sign {border: 1px solid;border-radius: 50%;cursor: pointer;font-size: 20px;padding: 5px;position: absolute;right: 15px;top: 15px;width: 28px;height: 28px;line-height: 17px;text-align: center;}
.pp_sizechart_container .pp_close-sign s-svg {height: .5em}

.js-menu__expanded {-webkit-transform: translateX(0); transform: translateX(0); visibility: visible; opacity: 1;display:block;}
  
body.size_content_open{overflow:hidden;}

.pplr-choose-product .pp_sizechart{background: rgba(0,0,0,.1);padding: 10px;margin-left: 15px;border-radius: 8px;text-transform: uppercase;color: #e36e37;cursor: pointer;}

/* #pp_sizechart .swatch_options input:checked+.swatch-element {border-color: #000;box-shadow: 0 0 0 1px #000;} */

.pp_sizechart_container {background-color: #eee; color: var(--color-text); display: block; height: 100%; overflow-y: auto; position: fixed; right: 0; top: 0 !important; transition: all .3s ease; width: 100%; z-index: 1010}
.pp_sizechart_container[aria-hidden=true] {opacity: 0; transform: translateX(100%); visibility: hidden}
/* .pp_sizechart_container .cf_close-sign {border: 1px solid; border-radius: 50%; cursor: pointer; font-size:22px; line-height: 0; padding: 5px; position: absolute; right: 2.29vw;
 top: 2.29vw; width:30px; height:30px; line-height:16px; text-align:center;}
.pp_sizechart_container .cf_close-sign s-svg {height: .5em} */
.pp_sizechart_container .pp_sizechart_title {margin:15px 0; text-align:center;display: flex; align-items: center; justify-content: center;}
.pp_sizechart_container .pp_sizechart_title img, .pp_sizechart_container .pp_sizechart_title svg{height:1.2em; margin-right:10px;}
.pp_sizechart_content-wrapper {display: flex;}
.pp_sizechart_layouts {display: flex; flex: 0 0 18%; flex-direction: column}
.pp_sizechart_layouts .pp_sizechart_layout {cursor: pointer; display: flex; flex-direction: column; font-size: 2.57vw; height: 22.86vw; justify-content: center; line-height: 1.1;
    padding: 0 3.14vw; position: relative; text-align: center;align-items: center; margin:0; border:0;}
.pp_sizechart_layouts .pp_sizechart_layout:before { background-color:#ffff00; content: ""; height: 100%; left: 0; opacity: 0; position: absolute; top: 0;
    transition: all .3s ease; width: 2px; z-index: 1}
.pp_sizechart_layouts .pp_sizechart_layout.active {background-color: #fff;border-color: transparent !important;box-shadow: none !important;}
.pp_sizechart_layouts .pp_sizechart_layout img {margin-bottom: 5px; /*width: 100%;*/}
.pp_sizechart_layouts .pp_sizechart_layout label{white-space: pre-wrap;}
.pp_sizechart_layouts .layout-icon {color: #c3c3c3; /*height: 9.14vw;*/ margin-bottom: 1.43vw; width: 13.14vw}
.pp_sizechart_content {background: #fff; flex: 1; height: -webkit-fit-content; height: -moz-fit-content; height: fit-content; padding: 3.14vw 2.29vw; position:relative;}
.pp_sizechart_content > p {font-size: 3.14vw; margin-top: 5.14vw}

/* .pp_sizechart_layouts .best_seller_text {color: #000;position: absolute;top: 0;left: 0;right: 0;background-color: #ffef30;font-weight: bold;text-transform: uppercase;font-size: 14px;} */
  
.pp_sizechart_images {line-height: 0; /*padding-top: 100%;*/ position: relative}

.pp_sizechart_images ul li{display: flex; align-items: center;}
.pp_sizechart_images ul li span{margin-right: 25px;}
.pp_sizechart_images ul li span.content_text{display: list-item;line-height: 21px;}
.pp_sizechart_images .pp_sizechart_background {z-index: 1;top: 0;width: 100%;}

.pp_sizechart_image_content{margin-top: 30px;}
.pp_sizechart_image_content ul{padding: 15px 0 0 0;}
.pp_sizechart_image_content ul p, .blanket_card_content p, .bundles_card p{margin: 0;}


.blanket_card_content p{font-weight: 600;}
.pp_sizechart_image_content .bundles_card ul{padding: 0 0 0 40px;}
.pp_sizechart_image_content .bundles_card ul li {display: list-item;}

.pp_sizechart_background_content{background-color: #ebebeb;padding: 35px 0 15px 10px;}
.pp_sizechart_images .pp_sizechart_background.canvas_card_img{margin-left: -10px;}

.pp_sizechart_content-wrapper .content_orange{color:#f35d2e;}
.desktop_only{display:inline-block !important;}
.mobile_only, .hidden{display:none !important;}

#shopify-section-1661256508f5058bdf .action_button.first_button{color: #000 !important;background-color: #ffffff !important;border-color: #ffffff !important;line-height: 0.2;} 

/* .304e10b3-0cad-4d9d-b586-e66b9453e5da_description{display:none;} */

.ju_Con{z-index: 3000 !important;}
.homepage-slideshow .headline{font-family: Pocket_Monk!important;text-shadow: none;-webkit-text-stroke: initial;}
.shoppay-note{width: 100%;margin: 15px 5px;font-weight: 600;text-transform: capitalize;}

@media (min-width:768px) {
.js-menu {width:600px; max-width: 85vw;}
.pp_sizechart_container .cf_close-sign {right:15px; top:22px}
.pp_sizechart_layouts .pp_sizechart_layout {font-size: 16px; height: 135px; padding: 0 15px; margin:0 !important;}
.pp_sizechart_layouts .pp_sizechart_layout label{line-height: 15px; padding:100px 0 0 !important;height: auto;width: 100px;}
.pp_sizechart_layouts .pp_sizechart_layout:before {width: 4px}
.pp_sizechart_content {padding:15px;}
.pp_sizechart_content > p {font-size: 16px; margin:10px 0;}

}

@media (min-width:1366px) {

.pp_sizechart_layouts:hover .pp_sizechart_layout:hover:before,
.pp_sizechart_layouts:not(:hover) .pp_sizechart_layout.active:before {opacity: 1}
.homepage-slideshow a.highlight-true, .homepage-slideshow input[type=button].highlight-true {margin-bottom: 0;}  
}
@media (max-width:1366px) {
.pp_sizechart_layouts .pp_sizechart_layout.active:before {opacity: 1}
  
.homepage-slideshow a.highlight-true, .homepage-slideshow input[type=button].highlight-true {font-size: 30px!important; padding: 30px 0;margin-bottom: 0;}
}
@media screen and (max-width:1080px) {
.homepage-slideshow a.highlight-true, .homepage-slideshow input[type=button].highlight-true {margin-bottom: 0;font-size: 25px!important;padding: 20px 25px;}
}
@media screen and (max-width:990px) {
 .homepage-slideshow a.highlight-true, .homepage-slideshow input[type=button].highlight-true {margin-bottom: 15px;}
}

@media screen and (max-width:890px) {
.pp_sizechart_container {overflow-y: scroll}  
}
