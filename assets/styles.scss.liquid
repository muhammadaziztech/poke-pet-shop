  img[src*='/no-image']{
    opacity: 0;
  }
  img[src*='/no-image'] ~ .thumbnail-overlay {
    opacity: 1 !important;
    .collection-details {
      opacity: 1;
      transform: none;
    }
  }
  img[src*='/no-image'] ~ .collection-info__caption {
    @include respond-to('medium'){
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      bottom: 0;
      right: 0;
    }
  }
