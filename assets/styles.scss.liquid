        @include flex-direction(row);
      }
    }
  }
  .featured-link--image:nth-of-type(4n+3) {
    @include flex-direction(row);
    @include respond-to('medium'){
      @include flex-direction(row-reverse);
    }
  }
  .featured-link--image:nth-of-type(4n+4) {
    @include flex-direction(row);
  }
}

