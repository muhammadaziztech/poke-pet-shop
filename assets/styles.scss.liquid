/* Product Quality Section */


@import url('https://fonts.googleapis.com/css2?family=Lato:wght@400;700&display=swap');
  
.our-quality-section{width: 100%; display: inline-block;}
.our-quality-section .page-width {max-width: 1500px;margin: 0px auto;}


.product_option{margin:20px 0;}

.fully-customizable-section .page-width {padding: 0;position: relative;text-align: center;}

.fully-customizable-section h2{font-size:32px;font-weight: 700;text-transform: uppercase;color: #005492; text-align: center; display: flex;
  	flex-direction: column;letter-spacing: 0;line-height: 1; margin-bottom: 60px;}

.fully-customizable-section h2 span{font-weight:400; font-size:26px; padding-bottom:10px;}
.fully-customizable-section .customizable-img img{max-width:100%;}




.quality-section-tabs,
.checkout-our-cards-section, .fully-customizable-section{padding:30px; background: #fff; border-radius: 10px; box-shadow: 0 2px 2px rgba(11,25,28,.1); margin: 0 0 40px;}


.quality-tabs-header{font-size:24px; font-weight:600; color:#005da1; display: flex; justify-content: center;}
  
.quality-tab-title {position: relative; display: flex; align-items: center; justify-content: center; /*flex: 0 1 100%;*/ cursor: pointer; transition: all 0.3s ease; 
  text-transform:capitalize;font-family: 'Lato', sans-serif;padding:5px 40px; margin:30px 0;}
.quality-tab-title.left{justify-content: end;padding-right: 0;}
.quality-tab-title.right{justify-content: flex-end;padding-right: 20px;}
.quality-tab-title span {position: relative}
.quality-tab-title span:after {content: ''; position: absolute; bottom:0; left: 50%; transform: translateX(-50%); height:2px; width: 100%;
  background-color: #f3ca2d; transition: all 0.25s ease; opacity:0;}
.quality-tab-title.tab-active {/*background-color: #1678fe;*/}
.quality-tab-title.tab-active span:after {opacity:1;}

.quality-tabs.page-width {padding: 0; position: relative; text-align:center;}
.quality-tab {padding: 0 0 8.57vw; opacity: 0; transition: opacity 0.5s eas;}
.quality-section-tabs .quality-tab h2{font-size:32px;font-weight: 700;text-transform: uppercase;color: #005492; text-align: center; display: flex;flex-direction: column;
    letter-spacing: 0;line-height: 1; margin-bottom: 80px;}
.quality-section-tabs .quality-tab h2 span{font-weight:400; font-size:26px; padding-bottom:10px;}

.quality-tab:not(.tab-active) {position: absolute; top: 0; left: 0; height: 100%; width: 100%; pointer-events: none;}
.quality-tab.tab-active {opacity: 1;}

.quality-featured {display: flex; justify-content: center; flex-wrap: wrap; font-size: 2.86vw; line-height: 1.3;}
/* .quality-featured .icon--check {color: #f18e8b;height: 3.71vw;width: 4.57vw;} */
.quality-featured img {width:40px; margin-right:5px;}

.quality-feature {flex: 0 1 34%; margin-bottom: 4.27vw; position:relative;display: flex; align-items: center;}
.quality-feature p {margin:0; padding:0; line-height:1.2; color:#005da1;font-family: 'Lato', sans-serif; text-transform:uppercase;}


.quality-tab-text { font-size:28px; line-height:1; font-weight:400; margin:40px 0 5vw;}
.quality-tab-text p{padding:0; margin:0; color:#005da1;font-family: 'Lato', sans-serif;}

.quality-tab-slider-wrapper {position: relative; padding: 0 4.29vw;}

.quality-tab-slider-wrapper .quality-slide {display: flex;flex-direction: column;align-items: center; justify-content: center; width: 100%;}
.quality-tab-slider-wrapper .quality-image-big {text-align: center; max-width: 800px;margin:0 auto 25px;}
.quality-tab-slider-wrapper .quality-slide-caption{display: flex; justify-content: center; width: 600px; margin: 0 auto; margin-bottom: 20px;}
.quality-slide-caption span{color: #005da1; text-transform: uppercase; display: flex; flex-wrap: nowrap; width: 100%;}
.quality-slide-caption span.with-bg{justify-content: center;padding-left:40px;}
.quality-slide-caption span.without-bg{justify-content: flex-end;padding-right: 10px;}

.quality-tab-slider-wrapper .quality-arrow { position: absolute; z-index: 10; top: calc(50% - 70px); height:70px; width: 25px; cursor: pointer;}
.quality-tab-slider-wrapper .quality-arrow:before {content: ''; display: block; position: absolute; left:0; top:0;
  font-size: 3.14vw; color: #999999; border-style: solid; border-width:35px 0 35px 15px; border-color: transparent transparent transparent #005ea1;}

.quality-tab-slider-wrapper .quality-image-small-top img,
  .quality-tab-slider-wrapper .quality-image-small-bottom img{border:3px solid #1678fe; border-radius:50%;}

.quality-tab-slider-wrapper .quality-tab-prev {left:12%; transform: rotate(180deg);}
.quality-tab-slider-wrapper .quality-tab-next {right:12%;}
.product-stat-card-features .quality-tab-next, .product-stat-card-featuresv2 .quality-tab-next {right:8%;}

/* Checkout cards section */
.checkout-our-cards-section h2{font-size:32px;font-weight: 700;text-transform: uppercase;color: #005492; text-align: center; display: flex;flex-direction: column;
